{
    "name": "Smark",
    "description": "best bookmark management tool",
    "version": "0.1",
    "manifest_version": 3,
    "omnibox": {
        "keyword": "sm"
    },
    "key": "",
    "oauth2": {
        "client_id": "980834304638-5ipdf6d20r77kosjjb1l8gj18hgr62c3.apps.googleusercontent.com",
        "scopes": [
            "email",
            "profile",
            "openid"
        ]
    },
    "icons": {
        "16": "./smark.png",
        "24": "./smark.png",
        "32": "./smark.png"
    },
    "action": {
        "default_icon": {
            "16": "./smark.png",
            "24": "./smark.png",
            "32": "./smark.png"
        },
        "default_title": "Smark",
        "default_popup": "./popup/index.html"
    },
    "background": {
        "service_worker": "./dist/background.js"
    },
    "options_page": "./options/index.html",
    "host_permissions": [
    ],
    "permissions": [
        "storage",
        "cookies",
        "unlimitedStorage",
        "tabs",
        "identity",
        "bookmarks",
        "clipboardRead",
        "clipboardWrite",
        "contextMenus"
    ],
    "content_security_policy": {
        "script-src": "self",
        "object-src": "none"
    }
}
