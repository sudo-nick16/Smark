{
    "name": "Smark",
    "description": "A bookmark manager for chrome",
    "version": "0.1",
    "manifest_version": 3,
    "commands": {
        "add-bookmark": {
            "suggested_key": "Alt+U",
            "description": "Add current tab to smark."
        }
    },
    "omnibox": {
        "keyword": "sm"
    },
    "key": "",
    "oauth2": {
        "client_id": "980834304638-5ipdf6d20r77kosjjb1l8gj18hgr62c3.apps.googleusercontent.com",
        "scopes": ["email", "profile", "openid"]
    },
    "icons": {
        "16": "./logo_64.png",
        "24": "./logo_128.png",
        "32": "./logo_512.png"
    },
    "action": {
        "default_icon": {
            "16": "./logo_64.png",
            "24": "./logo_128.png",
            "32": "./logo_512.png"
        },
        "default_title": "Smark",
        "default_popup": "./popup/index.html"
    },
    "background": {
        "service_worker": "./dist/background.js"
    },
    "options_page": "./options/index.html",
    "host_permissions": [],
    "permissions": [
        "storage",
        "unlimitedStorage",
        "tabs",
        "identity",
        "contextMenus"
    ],
    "content_security_policy": {
        "script-src": "self",
        "object-src": "none"
    }
}
